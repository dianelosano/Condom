        tools:layout_editor_absoluteX="84dp"
        tools:layout_editor_absoluteY="88dp"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="50dp"
        android:background="@drawable/obj_btn_radius_md"/>

    <Button
        android:id="@+id/button2"
